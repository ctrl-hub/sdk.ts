{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "./dist",
    "rootDir": "./services"
  },
  "include": [
    "services/**/*.ts",
    "services/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}